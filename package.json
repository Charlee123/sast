{
    "name": "Testing GHAS",
    "version": "1.1.3",
    "description": "Test GHAS",
    "author": "",
    "repository": {
        "type": "git",
        "url": "https://test/test/_git/ion-web"
    },
    "license": "UNLICENSED",
    "scripts": {
        "ng": "ng",
        "create-lib": "ng g lib boson-mass-upload --standalone",
        "build": "npm run build:dev",
        "build:prod": "npm run clean:dist && node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --configuration=prod",
        "build:qas": "npm run clean:dist && node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --configuration=qas",
        "build:dev": "npm run clean:dist && node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --configuration=dev",
        "clean:all": "npm-run-all --parallel clean:{cov,dist,docs,e2e}",
        "clean:cov": "rimraf {coverage,reports}",
        "clean:dist": "rimraf dist",
        "clean:docs": "rimraf documentation",
        "clean:e2e": "rimraf e2e-reports",
        "generate:docs": "npm run clean:docs && npx compodoc -p tsconfig.json -s",
        "lint": "eslint --ext .ts ./src ./e2e",
        "lint:fix:all": "npm-run-all --parallel lint:*:fix",
        "lint:css:fix": "stylelint \"**/*.css\" --ignore-path .gitignore --fix",
        "lint:js:fix": "eslint --ext .js . --ignore-path .gitignore --fix",
        "lint:md:fix": "remark . -o --quiet --frail --ignore-path .gitignore",
        "lint:scss:fix": "stylelint \"**/*.scss\" --syntax scss --ignore-path .gitignore --fix",
        "lint:ts:fix": "eslint --ext .ts ./src ./e2e --fix",
        "lint:report": "eslint --ext .ts ./src -f json -o eslint-report.json || exit 0",
        "prettier:base": "prettier --parser typescript",
        "prettier:html:check": "prettier src/**/*.html --parser html",
        "prettier:ng:check": "prettier src/**/*.component.html --parser angular",
        "prettier:ts:check": "npm run prettier:base -- --list-different \"src/**/*.ts\" \"e2e/**/*.ts\"",
        "prettier:fix:all": "npm-run-all --parallel prettier:*:fix",
        "prettier:template:fix": "npm run prettier:html:check -- --write src/**/*.html && npm run prettier:ng:check -- --write src/**/*.component.html",
        "prettier:ts:fix": "npm run prettier:base -- --write \"src/**/*.ts\" \"e2e/**/*.ts\"",
        "start": "ng serve",
        "start:dev": "ng serve --configuration=dev",
        "start:qas": "ng serve --configuration=qas",
        "test": "npm run clean:cov && ng test --browsers=ChromeHeadless --watch=false --code-coverage",
        "cover": "npm run clean:cov && ng test --browsers=ChromeHeadless --watch=false --code-coverage",
        "test:e2e": "ng e2e",
        "webdriver:update": "npx webdriver-manager update --ignore_ssl=true",
        "analyze": "ng build --configuration=prod --sourceMap && npx source-map-explorer ./dist/*-es2015.*.js"
    },
    "private": true,
    "dependencies": {
        "@ag-grid-community/angular": "~30.0.1",
